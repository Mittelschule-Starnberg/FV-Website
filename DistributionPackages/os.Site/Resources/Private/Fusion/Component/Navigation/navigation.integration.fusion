prototype(os.Site:Content.Navigation) < prototype(Neos.Fusion:Component) {

    menuItems = Neos.Neos:MenuItems {
        itemCollection = ${q(site).add(q(site).children('[instanceof Neos.Neos:Document]')).get()}
    }

    logo =  Neos.Neos:ImageUri {
        asset = ${q(node).property('logo')}
    }



    renderer = os.Site:Widget.Navigation {
        @apply.props = ${props}
    }
}
