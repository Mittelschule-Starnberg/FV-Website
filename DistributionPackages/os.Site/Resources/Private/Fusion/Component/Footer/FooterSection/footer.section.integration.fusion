prototype(os.Site:Content.FooterSection) < prototype(Neos.Fusion:Component) {
    renderer = afx`
        <section class="footer-section">
            <p class="footer-section-label">{props.title}</p>
            <Neos.Fusion:Loop items={props.content}>
                <Neos.Neos:NodeLink node={item} itemName="nodeItem">
                    {nodeItem}
                </Neos.Neos:NodeLink>
            </Neos.Fusion:Loop>
        </section>
    `
}
