prototype(os.Site:Content.LabelText) < prototype(Neos.Fusion:Component) {

    label = Neos.Neos:Editable {
        node=${node}
        property = 'label'
    }

    text = Neos.Neos:Editable {
        property = 'text'
        fallback = 'Error displaying headline'
    }
    @process.contentElementWrapping = Neos.Neos:ContentElementWrapping

    renderer = os.Site:Element.LabelText {
        @context.node = ${props}
    }
}
